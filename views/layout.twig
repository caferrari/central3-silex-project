<html>
    <head>
        <title>Title</title>
    </head>
    <link href="{{ app.request.baseUrl }}/css/styles.css" rel="stylesheet" media="screen" />
    <script src="http://code.jquery.com/jquery-1.8.1.min.js" type="text/javascript"></script>
    <script src="{{ app.request.baseUrl }}/js/scripts.js" type="text/javascript"></script>
    <body>
        <script type="text/javascript" charset="utf-8" src="http://barra.to.gov.br/topo.js?k={{ app.request.host }}"></script>
        <nav>
            <ul>
                <li><a href="{{ app.request.baseUrl }}">Inicio</a></li>
                <li><a href="{{ app.request.baseUrl }}/noticia">Noticias</a></li>
                <li><a href="{{ app.request.baseUrl }}/galeria">Galerias</a></li>
                {% for item in menu %}
                    <li><a href="{{ app.request.baseUrl ~ item.uri }}">{{ item.titulo }}</a></li>
                {% endfor %}
            </ul>
        </nav>
        {% block content %}
        {% endblock %}
        
        <script type="text/javascript" charset="utf-8" src="http://barra.to.gov.br/base.js?k={{ app.request.host }}"></script>
    </body>
</html>