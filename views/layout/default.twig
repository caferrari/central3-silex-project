<html>
    <head>
        <title>{{ site.nome }}</title>
        <link href="{{ app.request.baseUrl }}/css/styles.css" rel="stylesheet" media="screen" />
        <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.1/jquery.fancybox.css" type="text/css" media="screen" />
        <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.1/jquery.fancybox.pack.js" type="text/javascript"></script>
        <script src="{{ app.request.baseUrl }}/js/jquery.scroll4ever.js" type="text/javascript"></script>
        <script src="{{ app.request.baseUrl }}/js/scripts.js" type="text/javascript"></script>
    </head>
    <body>
        <script type="text/javascript" charset="utf-8" src="http://barra.to.gov.br/topo.js?k={{ app.request.host }}"></script>
        <div id="wrapper">
            <nav>
                <a href="{{ app.request.baseUrl }}">Inicio</a>
                <a href="{{ app.request.baseUrl }}/noticia">Noticias</a>
                <a href="{{ app.request.baseUrl }}/galeria">Galerias</a>
                {% for item in menu %}
                <a href="{{ app.request.baseUrl ~ item.uri }}">{{ item.titulo }}</a>
                {% endfor %}
            </nav>
            <div id="content" class="{{ app.request.attributes.get('_route')|replace({'.':' '}) }}">
            {% block content %}
            {% endblock %}
            </div>
            <sidebar>
                <script src="http://publicidade.to.gov.br/pub.php" type="text/javascript"></script>
                {% for banner in banners %}
                {{ banner.code|raw }}
                {% endfor %}
            </sidebar>
        </div>
        <script type="text/javascript" charset="utf-8" src="http://barra.to.gov.br/base.js?k={{ app.request.host }}"></script>
    </body>
</html>